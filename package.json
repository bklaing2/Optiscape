    "db:types": "supabase gen types typescript --linked --schema optiscape > src/lib/types/database.ts"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@iconify/json": "^2.2.202",
    "@neoconfetti/svelte": "^1.0.0",
