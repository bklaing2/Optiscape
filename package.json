    "vercel": "^34.2.8",
    "vite": "^5.0.0"
  },
  "type": "module",
  "dependencies": {
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.42.7",
    "epubjs": "^0.3.93",
    "r2-opds-js": "^1.0.43",
    "r2-utils-js": "^1.0.38",
    "url-safe-base64": "^1.3.0",
    "xmldom": "^0.6.0"
  }
